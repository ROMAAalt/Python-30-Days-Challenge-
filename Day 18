def count_divisors(n):
    cnt = 0
    i = 1
    while i * i <= n:
        if n % i == 0:
            cnt += 1  # i is a divisor
            if i != n // i:   # avoid double counting square root
                cnt += 1
        i += 1
    return cnt

if __name__ == "__main__":
    try:
        N = int(input().strip())
    except:
        N = int(input("Enter N: "))
    
    print(count_divisors(N))


