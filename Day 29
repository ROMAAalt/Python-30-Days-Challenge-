def fibonacci(n):
    if n == 0:
        return 0
    elif n == 1:
        return 1

    prev, curr = 0, 1
    for _ in range(2, n + 1):
        prev, curr = curr, prev + curr
    return curr

# Driver code
if __name__ == "__main__":
    try:
        n = int(input())
        if 0 <= n <= 1000:
            print(fibonacci(n))
        else:
            print("Input out of allowed range (0 ≤ n ≤ 1000).")
    except ValueError:
        print("Please enter a valid integer.")
